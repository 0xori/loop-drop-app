    "window-stream": "*",
    "ecstatic": "*",
    "tap-tempo": "~0.0.0",
    "brace": "~0.1.3",
    "ever": "~0.0.3",
    "watchify": "~0.5.0",
    "catw": "~0.2.0",
    "concat-stream": "~1.4.1",
    "dom-behavior": "~1.1.1",
    "create-element": "~0.2.3",
    "module-deps": "~1.2.2",
    "become": "~1.1.1",
